                <a href="/">
                    <%= userData.name%>
                </a>
                <%}else{%>
                    <a href="/signin">Login</a>
                    <a href="/signup">Register</a>
                    <%}%>


        </div>
    </div>
    <!-- Offcanvas Menu End -->

